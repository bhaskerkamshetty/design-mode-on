{
  "manifest_version": 3,
  "name": "Design Mode On",
  "version": "1.0",
  "description": "Turns on designMode for the current page, making it editable.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_title": "Make Page Editable",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}
